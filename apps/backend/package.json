{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bash -c 'source .venv/bin/activate && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000'",
    "start": "bash -c 'source .venv/bin/activate && python -m uvicorn main:app --host 0.0.0.0 --port 8000'",
    "install-deps": "pip install -r requirements.txt",
    "setup-venv": "python -m venv .venv && source .venv/bin/activate && pip install -r requirements.txt"
  }
} 